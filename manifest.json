{
  "manifest_version": 2,

  "name": "Open All",
  "description": "Click the button and make a rectangular selection on a page to open all links within it in new tabs.",
  "version": "1.0",
  
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Multiple Links",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab"
  ]
}